{
  "name": "pollux-lang",
  "displayName": "pollux-programming-language",
  "description": "Programming Language for Pollux Engine.",
  "version": "0.0.17",
  "publisher": "forcequality",
  "homepage": "https://github.com/GabyForceQ/pollux-lang-vscode/blob/master/README.md",
  "license": "SEE LICENSE IN LICENSE.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/GabyForceQ/pollux-lang-vscode"
  },
  "engines": {
    "vscode": "^1.35.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "preview": true,
  "icon": "icon.png",
  "contributes": {
    "languages": [
      {
        "id": "px",
        "extensions": [
          ".px"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "px",
        "scopeName": "source.px",
        "path": "./syntaxes/px.tmGrammar.json"
      }
    ],
    "snippets": [
      {
        "language": "px",
        "path": "./snippets/px.snippets.json"
      }
    ]
  },
  "dependencies": {
    "npm": "^6.9.2"
  },
  "devDependencies": {
    "js-yaml": "^3.13.1"
  }
}
